{
    "name": "signawebsolutions/yii2-modal-login",
    "description": "Make a button that open a Bootstrap4 Modal with the login form",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","login-form","modal-login"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Samir IZZA",
            "email": "samirmember@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap4": "~2.0.6"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "signa\\": ""
        }
    }
}
